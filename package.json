{
	"name": "payload-blog-typescript",
	"description": "Experfy career portal",
	"version": "1.0.0",
	"main": "dist/server.js",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
		"build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
		"build:server": "tsc",
		"build:server:silent": "tsc-silent -p tsconfig.json --suppress @",
		"build": "yarn copyfiles && yarn build:payload && yarn build:server:silent",
		"serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
		"copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
		"generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
		"generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
	},
	"dependencies": {
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@faceless-ui/css-grid": "^1.2.0",
		"@faceless-ui/modal": "^2.0.1",
		"@faceless-ui/mouse-info": "^1.3.0",
		"@faceless-ui/scroll-info": "^1.3.0",
		"@faceless-ui/window-info": "^2.1.1",
		"@hookform/error-message": "^2.0.0",
		"@material-ui/core": "^4.12.4",
		"@mui/icons-material": "^5.10.9",
		"@mui/material": "^5.10.11",
		"@mui/styles": "^5.10.10",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.2.0",
		"@types/node": "^18.11.7",
		"@types/react": "^18.0.24",
		"@types/react-dom": "^18.0.8",
		"ace-builds": "^1.12.3",
		"axios": "^0.27.2",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"grapesjs": "^0.19.5",
		"grapesjs-blocks-basic": "^1.0.1",
		"grapesjs-mjml": "^1.0.2",
		"grapesjs-plugin-toolbox": "^1.0.13",
		"grapesjs-preset-newsletter": "^0.2.21",
		"grapesjs-preset-webpage": "^1.0.2",
		"grapesjs-tailwind": "^1.0.8",
		"grapesjs-tui-image-editor": "^1.0.1",
		"material-ui-color": "^1.2.0",
		"material-ui-confirm": "^3.0.7",
		"payload": "^1.1.18",
		"react": "^18.2.0",
		"react-ace": "^10.1.0",
		"react-color": "^2.19.3",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.38.0",
		"react-router-dom": "^6.4.2",
		"sass": "^1.55.0",
		"sharp": "^0.31.1",
		"util": "^0.12.5",
		"web-vitals": "^3.0.4"
	},
	"devDependencies": {
		"@types/express": "^4.17.14",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"nodemon": "^2.0.20",
		"ts-node": "^10.9.1",
		"tsc-silent": "^1.2.2",
		"typescript": "^4.8.4"
	}
}
